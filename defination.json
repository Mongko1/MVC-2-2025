{
  "openapi": "3.1.0",
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "paths": {
    "/citizens": {
      "get": {
        "tags": [
          "Citizen Management"
        ],
        "summary": "Get All Citizens",
        "operationId": "get_all_citizens_citizens_get",
        "parameters": [
          {
            "name": "type",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                {
                  "enum": [
                    "General",
                    "Vulnerable",
                    "VIP"
                  ],
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "title": "Type"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CitizenResponse"
                  },
                  "title": "Response Get All Citizens Citizens Get"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/citizens/report": {
      "get": {
        "tags": [
          "Citizen Management"
        ],
        "summary": "Get All Citizen Reports",
        "operationId": "get_all_citizen_reports_citizens_report_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/CitizenReportResponse"
                  },
                  "type": "array",
                  "title": "Response Get All Citizen Reports Citizens Report Get"
                }
              }
            }
          }
        }
      }
    },
    "/citizens/register": {
      "post": {
        "tags": [
          "Citizen Management"
        ],
        "summary": "Register Citizen",
        "operationId": "register_citizen_citizens_register_post",
        "parameters": [
          {
            "name": "citizen_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Citizen Id"
            }
          },
          {
            "name": "age",
            "in": "query",
            "required": true,
            "schema": {
              "type": "integer",
              "title": "Age"
            }
          },
          {
            "name": "health_status",
            "in": "query",
            "required": true,
            "schema": {
              "enum": [
                "Normal",
                "Risk"
              ],
              "type": "string",
              "title": "Health Status"
            }
          },
          {
            "name": "citizen_type",
            "in": "query",
            "required": true,
            "schema": {
              "enum": [
                "General",
                "Vulnerable",
                "VIP"
              ],
              "type": "string",
              "title": "Citizen Type"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {

                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/shetlers": {
      "get": {
        "tags": [
          "Shelter Management"
        ],
        "summary": "Get All Shelters",
        "operationId": "get_all_shelters_shetlers_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/ShelterResponse"
                  },
                  "type": "array",
                  "title": "Response Get All Shelters Shetlers Get"
                }
              }
            }
          }
        }
      }
    },
