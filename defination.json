  "components": {
    "schemas": {
      "CitizenReportResponse": {
        "properties": {
          "citizen_id": {
            "type": "string",
            "title": "Citizen Id"
          },
          "shelter_id": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Shelter Id"
          },
          "check_in_date": {
            "anyOf": [
              {
                "type": "string",
                "format": "date-time"
              },
              {
                "type": "null"
              }
            ],
            "title": "Check In Date"
          }
        },
        "type": "object",
        "required": [
          "citizen_id"
        ],
        "title": "CitizenReportResponse"
      },
      "CitizenResponse": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          },
          "age": {
            "type": "integer",
            "title": "Age"
          },
          "health_status": {
            "$ref": "#/components/schemas/HealthStatus"
          },
          "registration_date": {
            "type": "string",
            "format": "date-time",
            "title": "Registration Date"
          },
          "citizen_type": {
            "$ref": "#/components/schemas/CitizenType"
          }
        },
        "type": "object",
        "required": [
          "id",
          "age",
          "health_status",
          "registration_date",
          "citizen_type"
        ],
        "title": "CitizenResponse"
      },
      "CitizenType": {
        "type": "string",
        "enum": [
          "General",
          "Vulnerable",
          "VIP"
        ],
        "title": "CitizenType"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "HealthStatus": {
        "type": "string",
        "enum": [
          "Normal",
          "Risk"
        ],
        "title": "HealthStatus"
      },
      "RiskLevel": {
        "type": "string",
        "enum": [
          "Low",
          "High"
        ],
        "title": "RiskLevel"
      },
      "ShelterResponse": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          },
          "capacity": {
            "type": "integer",
            "title": "Capacity"
          },
          "current_population": {
            "type": "integer",
            "title": "Current Population"
          },
          "risk_level": {
            "$ref": "#/components/schemas/RiskLevel"
          }
        },
        "type": "object",
        "required": [
          "id",
          "capacity",
          "current_population",
          "risk_level"
        ],
        "title": "ShelterResponse"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          },
          "input": {
            "title": "Input"
          },
          "ctx": {
            "type": "object",
            "title": "Context"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    }
  }
